block navbar
  #banner.navbar.navbar-fixed-top
    ul#top-links.container-fluid
      li#user-menu.menu
        a(href="http://www.rackspace.com/knowledge_center/getting-started/rackspace-private-cloud", target="_blank") Knowledge Center
        a(href="https://privatecloudforums.rackspace.com/", target="_blank") Forums
        a(href="#", data-bind="visible: dashboard.authCheck, click: dashboard.authLogout, text: dashboard.authUser() + ' (Logout)'")
    .navbar-inner
      .container-fluid
        #logo-wrapper
          a.brand(href='/')
            span
        ul#links.nav(data-bind='foreach: siteNav')
          li(data-bind='click: $parent.siteActive, css: {active: $parent.siteActive() === $data}')
            a(href='#', data-bind='text: name')
